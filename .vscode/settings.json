{
    "python.analysis.typeCheckingMode": "strict",
    "editor.codeActionsOnSave": {
        "source.organizeImports": "explicit",
        "source.fixAll": "explicit"
    },
    "editor.formatOnSave": true,
    "python.analysis.diagnosticSeverityOverrides": {
        "reportUnknownMemberType": "none",
        "reportMissingTypeStubs": "none",
    },
    "flake8.args": [
        "--max-line-length=120"
    ],
    "cmake.configureOnOpen": true,
    "cmake.sourceDirectory": "/workspaces/boticar-f1tenth-car-ros/src",
    "cmake.ignoreCMakeListsMissing": true,
    "workbench.colorCustomizations": {
        "editor.background": "#fcfcff",
        "editor.foreground": "#0c0505",
        "activityBar.background": "#b6b5c4",
        "sideBar.background": "#dcdcdc",
        "statusBar.background": "#d0ceec",
        "titleBar.activeBackground": "#5a55a8",
        "titleBar.activeForeground": "#faf9ff"
    },
    "python.autoComplete.extraPaths": [
        "/opt/ros/noetic/lib/python3/dist-packages",
        "${workspaceFolder}/src",
        "${workspaceFolder}/src/imu",
    ],
    "python.analysis.extraPaths": [
        "/opt/ros/noetic/lib/python3/dist-packages",
        "${workspaceFolder}/src",
    ],
    "cmake.debugConfig": {
        "miDebuggerPath": "/usr/bin/gdb"
    },
    "python.envFile": "${workspaceFolder}/.env",
    "python.testing.unittestArgs": [
        "-v",
        "-s",
        "./src",
        "-p",
        "*test*.py"
    ],
    "python.testing.autoTestDiscoverOnSaveEnabled": true,
    "python.testing.pytestEnabled": false,
    "python.testing.unittestEnabled": true,
    "testing.showCoverageInExplorer": true,
    "testing.followRunningTest": true,
    "testing.displayedCoveragePercent": "totalCoverage",
    "testing.coverageToolbarEnabled": false,
    "testing.countBadge": "failed",
    "cSpell.words": [
        "ctrlr"
    ],
    "terminal.integrated.profiles.linux": {
        "bash": {
            "path": "bash",
            "args": [
                "-c",
                "if [ -f /opt/ros/noetic/setup.bash ]; then source /opt/ros/noetic/setup.bash; fi; if [ -f ${workspaceFolder}/devel/setup.bash ]; then source ${workspaceFolder}/devel/setup.bash; fi; echo 'ROS environment initialized.'; exec bash -i"
            ]
        }
    },
    "terminal.integrated.defaultProfile.linux": "bash"
}